{
    "name": "gau",
    "private": true,
    "version": "1.0.1",
    "type": "module",
    "scripts": {
        "watch:theme": "cross-env NODE_ENV=development vite build --watch --config wp-content/themes/gau/vite.config.js",
        "watch:plugin": "cross-env NODE_ENV=development vite build --watch --config wp-content/plugins/gau-addons/vite.config.js",
        "watch": "concurrently \"yarn watch:theme\" \"yarn watch:plugin\"",
        "build:theme": "vite build --config wp-content/themes/gau/vite.config.js",
        "build:plugin": "vite build --config wp-content/plugins/gau-addons/vite.config.js",
        "build": "concurrently \"yarn build:theme\" \"yarn build:plugin\""
    },
    "browserslist": [
        "extends @wordpress/browserslist-config",
        "ios >= 9",
        "Firefox ESR",
        "not dead"
    ],
    "devDependencies": {
        "@vheemstra/vite-plugin-imagemin": "^2.0.0",
        "@wordpress/browserslist-config": "^6.11.0",
        "autoprefixer": "^10.4.20",
        "concurrently": "^9.1.0",
        "cross-env": "^7.0.3",
        "imagemin-mozjpeg": "^10.0.0",
        "imagemin-pngquant": "^10.0.0",
        "imagemin-webp": "^8.0.0",
        "prettier": "^3.3.3",
        "sass": "^1.80.7",
        "terser": "^5.36.0",
        "vite": "^5.4.11",
        "vite-plugin-static-copy": "^2.1.0"
    },
    "dependencies": {
        "@fancyapps/ui": "^5.0.36",
        "current-device": "^0.10.2",
        "ensemble-social-share": "git+https://github.com/loltgt/ensemble-social-share.git",
        "foundation-sites": "^6.9.0",
        "jquery": "^3.7.1",
        "js-cookie": "^3.0.5",
        "nanoid": "^5.0.8",
        "pace-js": "^1.2.4",
        "ramda": "^0.30.1",
        "select2": "^4.0.13",
        "simplebar": "^6.2.7",
        "sweetalert2": "^11.14.5",
        "swiper": "^11.1.14",
        "vanilla-lazyload": "^19.1.3"
    },
    "packageManager": "yarn@1.22.19+sha1.4ba7fc5c6e704fce2066ecbfb0b0d8976fe62447"
}
