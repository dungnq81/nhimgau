    ],
    "autoload": {
        "psr-4": {
            "HD\\": "inc/classes"
        }
    },
    "require": {
        "php": ">=8.2",
        "matthiasmullie/minify": "^1.3.73"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "config": {
        "optimize-autoloader": true,
        "classmap-authoritative": true,
        "preferred-install": "auto"
    },
    "scripts": {
        "post-install-cmd": [
            "composer dump-autoload -o"
        ],
        "post-update-cmd": [
            "composer dump-autoload -o"
        ]
    }
}
